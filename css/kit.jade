        .hash-e#cbb3694893dda7ad17fa70e114baabde cbb3694893dda7ad17fa70e114baabde
        mixin header(o)
          .header(class="#{o.className || ''}")
            .header__left
              - el = { text: 'statsocial' }
              - el.icon = { path: 'logo-icon'}
              include partials/icon-text

            .header__right
              - var className = 'header__right__request-demo rounded-btn--blue2 rounded-btn--hoverable'
              - el = { text: 'Request Demo', class: className, id: ''} 
              include partials/rounded-btn

              - el = { items: ['Log In', 'Sign Up'], class: 'header__right__actions', id: ''} 
              include partials/header-link-line

              - el = { class: 'input-icon--header', id: ''}
              - el.input = { id: 'input-'+o.inputId}
              - el.icon =  { path: 'search-icon' }
              include partials/input-icon

              - var el = { class: 'header__right__menu-icon', path: 'menu-icon' }
              include partials/icon
        +header({})

        .hash-e#c23cdad713ba9c41c6ed2c9bf8e5bac7 c23cdad713ba9c41c6ed2c9bf8e5bac7
